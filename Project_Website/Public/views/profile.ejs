</head>
<body>
  <div class='page'>    
    <a href='/edit_profile'>Edit Profile</a>
    <% if (data.gender) { %>
      <div id='displayName'><%= data.displayName %></div>
      <%= data.email %>
      <%= data.gender %>
      <%= data.school %>
      <%= data.course %>
    <% } else { %>
      <%= data.org_name %>
      <%= data.email %>
    <% } %>
    <% for (let i = 0; i < data.comp_type.length; i++) { %>
      <%= data.comp_type[i] %>
    <% } %>
    <%= data.message %>
    <div class='links'>
      <a href='/changePassword'>Change Your Password</a>
      <a href='/changeEmail'>Change Your Email</a>
    </div>
  </div>
</body>